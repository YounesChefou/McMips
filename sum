	.data
tab :	.word 4, 5, 8, 22

	.text

main :	la $t0, tab
	addi $a0, $0, 0 #resultat
	addi $t1, $0, 0 #compteur
	addi $t2, $0, 0 #variable temporaire
	
loop :	beq $t1, 4, end
	lw $t2, 0($t0)
	add $a0, $a0, $t2
	addi $t0, $t0, 4
	addi $t1, $t1, 1
	j loop
	
end :	li $v0, 1
	syscall
	li $v0, 10
	syscall
		